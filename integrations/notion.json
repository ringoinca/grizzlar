{
  "name": "Notion",
  "image": "https://upload.wikimedia.org/wikipedia/commons/thumb/e/e9/Notion-logo.svg/1920px-Notion-logo.svg.png",
  "auth": {
    "authorizationURL": "https://api.notion.com/v1/oauth/authorize",
    "tokenURL": "https://api.notion.com/v1/oauth/token",
    "authType": "OAUTH2",
    "authorizationParams": { "response_type": "code", "owner": "user" }
  },
  "request": {
    "baseURL": "https://api.notion.com/v1/",
    "headers": {
      "Accept": "application/json",
      "Authorization": "Bearer ${auth.accessToken}",
      "User-Agent": "Pizzly"
    }
  }
}
